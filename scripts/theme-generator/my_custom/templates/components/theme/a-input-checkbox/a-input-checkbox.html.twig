<div class="a-input-checkbox h-boolean">
  <input {{ attributes.addClass('h-boolean__element') }}/>
  {% include '@component/a-icon/a-icon.html.twig' with {
    attributes: create_attribute().addClass('h-boolean__icon', 'h-boolean__icon--empty'),
    icon: 'checkbox-empty',
    my_custom_svg_sprite: my_custom_svg_sprite,
  } only %}
  {% include '@component/a-icon/a-icon.html.twig' with {
    attributes: create_attribute().addClass('h-boolean__icon', 'h-boolean__icon--filled'),
    icon: 'checkbox-filled',
    my_custom_svg_sprite: my_custom_svg_sprite,
  } only %}
  {{- children }}
</div>
