<div class="a-input-radio">
  <input {{ attributes.addClass('a-input-radio__element') }}/>
  {% include '@component/a-icon/a-icon.html.twig' with {
    attributes: create_attribute().addClass('a-input-radio__icon', 'a-input-radio__icon--empty'),
    icon: 'radio-empty',
    super_custom_theme_svg_sprite: super_custom_theme_svg_sprite,
  } only %}
  {% include '@component/a-icon/a-icon.html.twig' with {
    attributes: create_attribute().addClass('a-input-radio__icon', 'a-input-radio__icon--filled'),
    icon: 'radio-filled',
    super_custom_theme_svg_sprite: super_custom_theme_svg_sprite,
  } only %}
  {{- children }}
</div>
